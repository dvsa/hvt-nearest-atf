{# Example that changes every setting in the template #}

{% from "govuk-frontend/govuk/components/back-link/macro.njk" import govukBackLink %}
{% from "govuk-frontend/govuk/components/skip-link/macro.njk" import govukSkipLink %}
{% from "govuk-frontend/govuk/components/header/macro.njk" import govukHeader %}
{% from "govuk-frontend/govuk/components/phase-banner/macro.njk" import govukPhaseBanner %}
{% from "govuk-frontend/govuk/components/footer/macro.njk" import govukFooter %}
{% from "includes/analytics/macro.njk" import bodyscript as gaBodyScript %}

{% set htmlClasses = "app-html-class" %}
{% set htmlLang = "en" %}
{% set assetPath = "" %}
{% set assetUrl = "<YOUR-DOMAIN>" %}
{% set themeColor = "blue" %}
{% set bodyClasses = "app-body-class" %}
{% set bodyAttributes = {
  'data-test': 'My value',
  'data-other': 'report:details'
} %}
{% set containerClasses = "app-width-container" %}
<!DOCTYPE html>
<html lang="en" class="govuk-template app-html-class">

<head>
  <meta charset="utf-8">

<title>{% block pageTitle %}Find a test centre for an HGV, bus or trailer MOT{% endblock %}</title>

{% block headIcons %}
  <link rel="shortcut icon" href="/assets/images/favicon.ico" type="image/x-icon" />
{% endblock %}

{% block head %}
  {% include "includes/head.njk" %}
{% endblock %}
</head>
<body class="govuk-template__body app-body-class" data-test="My value" data-other="report:details">
  <script>
    document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');

  </script>

{% include "includes/cookie-manager/cookie-success-banner.njk" %}
{% include "includes/cookie-manager/cookie-success-all-banner.njk" %}
{% include "includes/cookie-manager/cookie-banner.njk" %}
{% block bodyStart %}
{% endblock %}

{% block skipLink %}
  {{ govukSkipLink({
    href: "#main-content",
    text: "Skip to main content"
  }) }}
{% endblock %}

{% block header %}

  {{ govukHeader({
    homepageUrl: "https://www.gov.uk",
    containerClasses: "govuk-width-container",
    serviceName: "Find a test centre for an HGV, bus or trailer MOT",
    serviceUrl: "https://find-test-centre-hgv-bus-trailer-mot.service.gov.uk"
  }) }}
  
{% endblock %}
<div class="govuk-width-container ">
{% set mainClasses = "app-main-class" %}

{% block beforeContent %}
  {{ govukPhaseBanner({
    tag: {
      text: "alpha"
    },
    html: 'This is a new service â€“ your <a class="govuk-link" href="https://www.gov.uk/done/find-mot-test-centre-hgv-bus-trailer">feedback</a> will help us to improve it.'
  }) }}
{% endblock %}

{% block backLink %}
{% endblock %}

 <main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content" role="main">
{% block main %}
{% endblock %}

{% block content %}
  <h1 class="govuk-heading-xl">Customised page template {{message}}</h1>
{% endblock %}
</main>
</div>
{% block footer %}
  {{ govukFooter({
    meta: {
      items: [
        {
          href: "/accessibility",
          text: "Accessibility"
        },
        {
          href: "/privacy",
          text: "Privacy"
        },
        {
          href: "/cookies",
          text: "Cookies"
        }
      ]
    }
  }) }}
{% endblock %}

{{ gaBodyScript(analytics) }}

{% block bodyEnd %}
{% include "includes/footerjs.njk" %}

{% endblock %}
</body>
</html>
